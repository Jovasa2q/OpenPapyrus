<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LAYOUT_STQFACE_IDS"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <com.google.android.material.textfield.TextInputLayout
        style="@style/InputLine"
        android:id="@+id/CTL_STQFACE_GLN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@{styloqface_gln}">
        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
    </com.google.android.material.textfield.TextInputLayout>
    <com.google.android.material.textfield.TextInputLayout
        style="@style/InputLine"
        android:id="@+id/CTL_STQFACE_RUINN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@{styloqface_ruinn}">
        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
    </com.google.android.material.textfield.TextInputLayout>
    <com.google.android.material.textfield.TextInputLayout
        style="@style/InputLine"
        android:id="@+id/CTL_STQFACE_RUKPP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@{styloqface_rukpp}">
        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
    </com.google.android.material.textfield.TextInputLayout>
    <com.google.android.material.textfield.TextInputLayout
        style="@style/InputLine"
        android:id="@+id/CTL_STQFACE_RUSNILS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@{styloqface_rusnils}">
        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
    </com.google.android.material.textfield.TextInputLayout>
</LinearLayout>