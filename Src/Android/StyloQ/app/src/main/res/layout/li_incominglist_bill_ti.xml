<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" style="@style/ListItemRegular"
    android:id="@+id/CTL_INCOMINGLIST_BILL_TI"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
    <TextView
        android:id="@+id/CTL_DOCUMENT_TI_GOODSNAME"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_marginLeft="2dp"
        android:textSize="12sp"
        android:textFontWeight="500"
        android:textColor="@color/TextPrimary"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <View style="@style/FakeView"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <View style="@style/FakeView"/>
                <TextView
                    android:id="@+id/CTL_DOCUMENT_TI_QTTY"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:minWidth="40dp"
                    android:textSize="12sp"
                    android:textFontWeight="600"
                    android:layout_marginRight="8dp"
                    android:textAlignment="viewEnd"
                    android:textColor="@color/TextPrimary"
                    />
                <TextView
                    android:id="@+id/CTL_DOCUMENT_TI_PRICE"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:minWidth="40dp"
                    android:textSize="12sp"
                    android:textFontWeight="600"
                    android:layout_marginRight="8dp"
                    android:textAlignment="viewEnd"
                    android:textColor="@color/TextPrimary"
                    />
                <TextView
                    android:id="@+id/CTL_DOCUMENT_TI_AMOUNT"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:minWidth="40dp"
                    android:textSize="14sp"
                    android:textFontWeight="600"
                    android:layout_marginRight="8dp"
                    android:textAlignment="viewEnd"
                    android:textColor="@color/Accent"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView android:id="@+id/CTL_DOCUMENT_TI_ACCEPTED_STATUS"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginBottom="4dp"/>
                <TextView android:id="@+id/CTL_DOCUMENT_TI_ACCEPTED_TEXT"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:textSize="12sp"
                    android:textFontWeight="400"
                    android:textColor="@color/TextPrimary"
                    android:text="@{accepted_neut}:"/>
                <View style="@style/FakeView"/>
                <TextView
                    android:id="@+id/CTL_DOCUMENT_TI_ACCPTED_QTTY"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:minWidth="40dp"
                    android:textSize="12sp"
                    android:textFontWeight="600"
                    android:layout_marginRight="8dp"
                    android:textAlignment="viewEnd"
                    android:textColor="@color/TextPrimary"
                    />
                <TextView
                    android:id="@+id/CTL_DOCUMENT_TI_ACCPTED_PRICE"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:minWidth="40dp"
                    android:textSize="12sp"
                    android:textFontWeight="600"
                    android:layout_marginRight="8dp"
                    android:textAlignment="viewEnd"
                    android:textColor="@color/TextPrimary"
                    />
                <TextView
                    android:id="@+id/CTL_DOCUMENT_TI_ACCPTED_AMOUNT"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:minWidth="40dp"
                    android:textSize="14sp"
                    android:textFontWeight="600"
                    android:layout_marginRight="8dp"
                    android:textAlignment="viewEnd"
                    android:textColor="@color/Accent"
                    />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>