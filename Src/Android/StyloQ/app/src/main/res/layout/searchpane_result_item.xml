<?xml version="1.0" encoding="utf-8"?>
<LinearLayout style="@style/ListItemRegular" xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/searchPaneResultItem"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
    <TextView
        android:id="@+id/LVITEM_GENERICNAME"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:textColor="@color/PrimaryText"
        />
    <ListView
        android:id="@+id/searchPaneTerminalListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="2dp"
        android:layout_marginRight="2dp"
        android:divider="@android:color/transparent"
        android:dividerHeight="2dp"
        >
    </ListView>
</LinearLayout>